{
  "ci": {
    "collect": {
      "url": ["/"],
      "staticDistDir": "./docs/performance/static",
      "numberOfRuns": 1,
      "settings": {
        "chromeFlags": ["--headless=new", "--disable-dev-shm-usage", "--no-sandbox"],
        "locale": "fr"
      }
    },
    "assert": {
      "assertions": {}
    },
    "upload": {
      "target": "filesystem",
      "outputDir": "./docs/performance/raw"
    }
  }
}
